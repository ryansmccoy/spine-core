# Market Spine Full - Environment Configuration
# Copy to .env and adjust values

# Database (TimescaleDB)
DATABASE_URL=postgresql://spine:spine@localhost:5432/spine

# Redis
REDIS_URL=redis://localhost:6379/0

# Celery
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1

# Execution lanes (comma-separated)
EXECUTION_LANES=fast,batch

# Backend selector (celery, local, temporal_stub, dagster_stub)
EXECUTION_BACKEND=celery

# S3 Storage (optional)
# S3_BUCKET=my-bucket
# S3_PREFIX=market-spine/
# AWS_ACCESS_KEY_ID=xxx
# AWS_SECRET_ACCESS_KEY=xxx
# AWS_REGION=us-east-1

# Observability
LOG_LEVEL=INFO
LOG_JSON=true
TRACING_ENABLED=false
METRICS_ENABLED=true

# OpenTelemetry (if TRACING_ENABLED=true)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=market-spine

# API
API_HOST=0.0.0.0
API_PORT=8000
